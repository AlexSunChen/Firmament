{
	"image": "asset/tilesheet/{{texture.fullName}}"
	,"tiles":[
		{% for sprite in sprites %}
		{
			"label": "{{sprite.trimmedName}}"
			,"width": {{sprite.frameRect.width}}
			,"height":{{sprite.frameRect.height}}
			,"topLeft":{"x":{{sprite.frameRect.x}},"y": {{sprite.frameRect.y}}}
		},
		{% endfor %}
	]
}